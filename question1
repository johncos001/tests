a = [random.randint(0,100) for r in range(random.randint(3,20))]
b = [random.randint(0,100) for r in range(random.randint(3,20))]

results = []
for i in a:
    if i in b:
        results.append(i)
        
for j in b:
    if j in a:
        results.append(j)
        
results = set(results) 
  
print(a)
print(b)    
print(results)
